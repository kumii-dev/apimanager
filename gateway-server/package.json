{
  "name": "kumii-gateway-server",
  "version": "1.0.0",
  "description": "KUMII API Gateway - ISO 27001 Compliant, Security-First",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "vercel-build": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "jest --testPathPattern=security",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix"
  },
  "keywords": [
    "api-gateway",
    "security",
    "iso27001",
    "zero-trust"
  ],
  "author": "KUMII Platform",
  "license": "PROPRIETARY",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.8",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "pino": "^8.17.2",
    "pino-http": "^9.0.0",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vercel/node": "^3.2.29",
    "eslint": "^8.56.0",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "pino-pretty": "^13.1.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
